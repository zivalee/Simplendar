<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ziva.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/dark">
        <activity
            android:name=".MainActivity"
            android:theme="@style/dark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".add"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".add_calorie"
            android:parentActivityName=".add" />
        <activity
            android:name=".add_expense"
            android:parentActivityName=".add" />
        <activity
            android:name=".add_water"
            android:parentActivityName=".add" />
        <activity
            android:name=".add_idea"
            android:parentActivityName=".add" />
        <activity
            android:name=".view_calorie"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".view_expense"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".view_water"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".view_idea"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>